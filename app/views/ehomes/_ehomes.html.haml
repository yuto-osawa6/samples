.planning
  .swiper-container2.planing__grid__uti
    .swiper-wrapper
      - ehomes.eehomes.each_with_index do |i,t|
        .swiper-slide
          .planing__grid.milkkandy__box__service
            .planing__grid__box
              .planning__image.playzoon2{"data-youtube_id2" => "#{i.id}"}
                - if i.image?
                  = link_to eehomes_youtube_path() do
                    = image_tag i.image.url
                    .planning__players
                      = icon("far","play-circle",class:"players")
              .planning__price.planning__price2
              
                = safe_join(i.discribe.split("\n"),tag(:br))
              .planning__title
                = i.title

